{
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "https://microjam-backend.onrender.com/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://microjam-backend.onrender.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}